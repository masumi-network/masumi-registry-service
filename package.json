{
    "name": "masumi-registry-service",
    "version": "1.0.1",
    "description": "A Payment credit service for MASUMI AI Nodes",
    "main": "dist/index.js",
    "author": "NMKR",
    "license": "MIT",
    "type": "commonjs",
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "_moduleAliases": {
        "@": "./dist"
    },
    "scripts": {
        "lint": "eslint --fix --quiet --config eslint.config.mjs",
        "format": "prettier --log-level silent --write src/**/*.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:seed": "npx prisma db seed",
        "dev": "tsx watch ./src/index.ts",
        "build": "npx prisma generate && npx pkgroll",
        "start": "NODE_ENV=production && node ./dist/index.js",
        "swagger-json": "tsx ./src/utils/swagger-generator/cli.ts"
    },
    "dependencies": {
        "@asteasolutions/zod-to-openapi": "^7.3.0",
        "@blockfrost/blockfrost-js": "^5.7.0",
        "@jest/types": "^29.6.3",
        "@meshsdk/core": "^1.8.9",
        "@paralleldrive/cuid2": "^2.2.2",
        "@prisma/client": "^6.2.1",
        "async-sema": "^3.1.1",
        "dotenv": "^16.4.7",
        "express": "^5.0.1",
        "express-zod-api": "^21.11.0",
        "http-errors": "^2.0.0",
        "http-status-codes": "^2.3.0",
        "node": "^18.19.0",
        "node-cron": "^3.0.3",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.17.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^18.19.0",
        "@types/node-cron": "^3.0.11",
        "@types/swagger-ui-express": "^4.1.7",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "pkgroll": "^2.6.1",
        "prettier": "^3.4.2",
        "prisma": "^6.2.1",
        "ts-jest": "^29.2.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.19.1"
    }
}
